{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"jQuery": false,
		"mediaWiki": false
	},
	"rules": {
		"no-jquery/no-global-selector": "warn",
		"no-jquery/no-extend": "warn",
		"es-x/no-object-assign": "warn",
		"prefer-const": "warn"
	}
}
